extends ../layout

block title
  | #{status} - #{t('authentication.error')}
block content
  .row
    .col-lg-6
      !=t('authentication.error_text', { postProcess: 'pug' })
      p #{t('error.sorry')}
    .col-lg-6
      h3 #{status}
      p #{error}
      h4 Stacktrace:
      p #{removeServerpaths(error.stack)}
